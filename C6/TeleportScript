-- Put this Script inside the Part
-- Make sure the Part has a ProximityPrompt inside it

local teleportPart = script.Parent
local prompt = teleportPart:WaitForChild("ProximityPrompt")

-- Destination position (edit this)
local destination = Vector3.new(50, 10, 50)

-- When the player triggers the prompt
prompt.Triggered:Connect(function(player)
	local character = player.Character
	if character then
		local hrp = character:FindFirstChild("HumanoidRootPart")
		if hrp then
			hrp.CFrame = CFrame.new(destination)
		end
	end
end)
